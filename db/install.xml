<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="theme/bambuco/db" VERSION="20250330" COMMENT="XMLDB file for Moodle theme/bambuco"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="theme_bambuco_subthemes" COMMENT="Subthemes for Bambuco theme">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="name" TYPE="char" LENGTH="63" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="idnumber" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="homeurl" TYPE="char" LENGTH="511" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="customsettings" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="idnumber" UNIQUE="true" FIELDS="idnumber"/>
      </INDEXES>
    </TABLE>
  </TABLES>
</XMLDB>